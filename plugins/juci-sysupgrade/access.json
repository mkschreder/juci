{
	"juci-sysupgrade": {
		"description": "JUCI Sysupgrade Interface module",
		"read": {
			"ubus": {
				"/juci/system.upgrade": [
					"check", 
					"test", 
					"start", 
					"clean"
				]
			}, 
			"uci": [ "system" ]
		}, 
		"write": {
			"uci": [ "system" ],
			"ubus": {
				"/juci/system.upgrade": [
					"check", 
					"test", 
					"start", 
					"clean"
				]
			}
		}
	}
}
